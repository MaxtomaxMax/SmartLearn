// 文档教程: https://uniapp.dcloud.net.cn/uniCloud/schema
{
	"bsonType": "object",
	"required": ["email", "password"],
	"permission": {
		"read": true,
		"create": false,
		"update": false,
		"delete": false
	},
	"properties": {
		"_id": {
			"description": "ID，系统自动生成"
		},
		"email":{
			"bsonType": "string",
			"title": "邮箱",
			"trim":"both"
		},
		"password":{
			"bsonType": "string",
			"title": "密码",
			"trim": "both"
		},
		"username":{
			"bsonType": "string",
			"title": "用户名"
		}
		
	}
}